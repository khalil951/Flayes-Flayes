    <script>
        const roomCategoriesCanvas = document.querySelector('[data-controller="symfony--ux-chartjs--chart"]');
        roomCategoriesCanvas.width = 400;
        roomCategoriesCanvas.height = 400;
        const chartDataString = roomCategoriesCanvas.getAttribute('data-symfony--ux-chartjs--chart-view-value');
        const chartData = JSON.parse(chartDataString);

        new Chart(roomCategoriesCanvas, {
            type: chartData.type,
            data: chartData.data,
            options: chartData.options
        });
    </script>
